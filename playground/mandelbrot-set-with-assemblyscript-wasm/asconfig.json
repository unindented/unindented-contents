{
  "targets": {
    "debug": {
      "binaryFile": "mandelbrot.debug.wasm",
      "textFile": "mandelbrot.debug.wat",
      "sourceMap": true,
      "debug": true
    },
    "release": {
      "binaryFile": "mandelbrot.release.wasm",
      "textFile": "mandelbrot.release.wat",
      "sourceMap": true,
      "optimizeLevel": 3,
      "shrinkLevel": 0,
      "converge": false,
      "noAssert": false
    }
  },
  "options": {
    "importMemory": true,
    "runtime": "stub",
    "use": "Math=JSMath"
  }
}
